{
  "name": "docs-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "tina-dev": "TINA_PUBLIC_IS_LOCAL=true tsx tina/server.ts",
    "docusaurus": "docusaurus",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "docs": "docusaurus start --port 3232 --host 0.0.0.0",
    "cms": "cross-env TINA_PUBLIC_CMS_URL=https://vault.thedigitalgroup.com tinacms dev --port 4001 --datalayer-port 9100 -c \"npm --prefix my-tina-app run dev -- -p 3100 -H 0.0.0.0\"",
    "dev": "concurrently -k -n DOCS,CMS -c green,cyan \"npm run docs\" \"npm run cms\""
  },
  "dependencies": {
    "@docusaurus/core": "3.8.0",
    "@docusaurus/preset-classic": "3.8.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "next": "^15.3.3",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-tinacms-editor": "^0.53.26",
    "redoc": "^2.5.0",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.0",
    "@docusaurus/tsconfig": "3.8.0",
    "@docusaurus/types": "3.8.0",
    "@tinacms/cli": "^1.9.8",
    "@tinacms/datalayer": "^1.3.18",
    "@types/node": "^22.15.29",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "tinacms": "^2.7.8",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
